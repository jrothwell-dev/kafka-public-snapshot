{
  "title": "Test Results Dashboard",
  "tags": ["tests", "ci", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 3, "w": 24, "x": 0, "y": 0},
      "type": "text",
      "title": "Test Health Overview",
      "options": {
        "content": "## Test Results Dashboard\n\nTrack test execution metrics across all services. Metrics are pushed to Prometheus Pushgateway when tests run via `make test-with-metrics`.",
        "mode": "markdown"
      }
    },
    {
      "id": 2,
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 3},
      "type": "stat",
      "title": "Total Tests Passed",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "sum(wwcc_tests_total{status=\"passed\"})",
          "refId": "A",
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto"
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 3},
      "type": "stat",
      "title": "Total Tests Failed",
