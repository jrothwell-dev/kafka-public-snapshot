{
  "title": "WWCC Pipeline Overview",
  "tags": ["wwcc", "kafka", "pipeline"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "type": "stat",
      "title": "raw.safetyculture.credentials",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "sum(kafka_topic_partition_current_offset{topic=\"raw.safetyculture.credentials\"})",
          "refId": "A",
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "type": "stat",
