    {
      "id": 6,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 7},
      "type": "timeseries",
      "title": "Message Flow Rate (messages per minute)",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "rate(kafka_topic_partition_current_offset{topic=\"raw.safetyculture.credentials\"}[1m]) * 60",
          "refId": "A",
          "legendFormat": "raw.safetyculture.credentials",
          "datasource": {"type": "prometheus"}
        },
        {
          "expr": "rate(kafka_topic_partition_current_offset{topic=\"processed.wwcc.status\"}[1m]) * 60",
          "refId": "B",
          "legendFormat": "processed.wwcc.status",
          "datasource": {"type": "prometheus"}
        },
        {
          "expr": "rate(kafka_topic_partition_current_offset{topic=\"commands.notifications\"}[1m]) * 60",
          "refId": "C",
          "legendFormat": "commands.notifications",
          "datasource": {"type": "prometheus"}
        },
        {
          "expr": "rate(kafka_topic_partition_current_offset{topic=\"events.notifications.sent\"}[1m]) * 60",
          "refId": "D",
          "legendFormat": "events.notifications.sent",
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {"tooltip": false, "viz": false, "legend": false},
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {"type": "linear"},
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"}
            ]
          },
          "unit": "msg/min"
        }
      }
    },
