              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "type": "stat",
      "title": "events.compliance.issues",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "sum(kafka_topic_partition_current_offset{topic=\"events.compliance.issues\"})",
          "refId": "A",
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "type": "stat",
      "title": "commands.notifications",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "sum(kafka_topic_partition_current_offset{topic=\"commands.notifications\"})",
          "refId": "A",
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "type": "timeseries",
      "title": "Consumer Group Lag",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "kafka_consumer_lag_sum",
          "refId": "A",
          "legendFormat": "{{consumer_group}}",
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {"tooltip": false, "viz": false, "legend": false},
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {"type": "linear"},
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 100, "color": "red"}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "type": "table",
      "title": "Service Container Status",
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "up{job=~\"kafka-exporter|postgres-exporter|redis-exporter\"}",
          "refId": "A",
          "format": "table",
          "instant": true,
          "datasource": {"type": "prometheus"}
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [
            {
              "options": {
                "0": {"text": "DOWN", "color": "red"},
                "1": {"text": "UP", "color": "green"}
              },
              "type": "value"
            }
          ]
        },
        "overrides": []
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true
            },
            "indexByName": {},
            "renameByName": {
              "job": "Service",
              "Value": "Status"
            }
          }
        }
      ]
    },
    {
      "id": 7,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
      "type": "logs",
      "title": "Recent Log Entries",
      "datasource": {"type": "loki"},
      "targets": [
        {
          "expr": "{container_name=~\"safetyculture-poller|wwcc-transformer|compliance-notification-router\"}",
          "refId": "A",
          "datasource": {"type": "loki"}
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      }
    }
  ],
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
